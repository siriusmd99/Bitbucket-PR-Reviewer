{
	"name" : "Bitbucket PR Reviewer",
	"version" : "1.0",
	"icons": 
	{ 
		"16": "icon16.png",
        "32": "icon32.png",
        "192": "icon192.png" 
	},
	"manifest_version" : 2,
	"browser_action": 
	{
		"default_popup": "popup.html"
    },
	"content_scripts" : 
	[
		{
			"matches" : ["<all_urls>"],
			"js" : ["prReviewer.js"]
		}
	]
}